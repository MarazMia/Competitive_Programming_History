    import math
    for _ in range(int(input())):
        n=int(input())
        arr=list(map(int,input().split()))
        #arr=list(set(arr))
        arr.sort()
        #print(arr)
        mini=arr[0]
        for i in range(1,len(arr)):
            mini=max(mini,arr[i]-arr[i-1])
        print(mini)
        
