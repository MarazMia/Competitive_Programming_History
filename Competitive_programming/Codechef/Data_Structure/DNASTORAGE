from cmath import e
import math
from re import X
    
def main():
    for _ in range(int(input())):
        d={'00':'A','01':'T',"10":'C',"11":"G"}
        ans=""
        n=int(input())
        a=list(input())
        i=1
        while i<n:
            ans+=d[a[i-1]+a[i]]
            i+=2
        print(ans)     
if __name__ == '__main__':
    
    main()

        