#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long t,ans,n;
    cin>>t;
    while(t--)
    {
        cin>>n;
        ans=n/5;
        n=n/5;
        while(n>=5)
        {
            ans=ans+(n/5);
            n=n/5;
        }
        cout<<ans<<endl;
    }
    return 0;
}
